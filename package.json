{
  "name": "focus-components",
  "version": "0.7.0",
  "description": "Focus component repository.",
  "main": "lib/index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha src/**/__tests__/**/*.js",
    "test:watch": "node ./node_modules/mocha/bin/mocha src/**/__tests__/**/*.js -w",
    "build:server": "npm run material:override && node ./node_modules/babel/bin/babel.js src --out-dir lib",
    "build:browser": "npm run material:override && npm run webpack:production",
    "start": "npm run webpack:showcase",
    "build": "npm run showcase:components && npm run build:server && npm run build:browser",
    "prepublish": "npm run build",
    "showcase:components": "node scripts/components-scanner.js",
    "material:override": "node scripts/mdl-variables-copy.js",
    "webpack:dev": "npm run showcase:components && node server.js",
    "webpack:showcase": "npm run showcase:components && cd src/showcase && node server.js",
    "webpack:production": "npm run material:override && node ./node_modules/webpack/bin/webpack.js --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KleeGroup/focus-components.git"
  },
  "keywords": [
    "react",
    "focus"
  ],
  "author": "Focus Team <focus@kleegroup.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KleeGroup/focus-components/issues"
  },
  "homepage": "https://github.com/KleeGroup/focus-components",
  "dependencies": {
    "focus-core": "^0.11.0-beta4",
    "i18next-client": "^1.10.2",
    "immutable": "^3.7.4",
    "lodash": "^3.10.1",
    "material-design-icons": "^2.0.0",
    "material-design-lite": "1.0.4-2",
    "moment": "^2.10.6",
    "object-assign": "^2.0.0",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-eslint": "4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-runtime": "^1.0.7",
    "babel-runtime": "^5.8.24",
    "backbone": "^1.2.3",
    "brace": "^0.5.1",
    "chai": "^3.2.0",
    "chai-subset": "^1.0.1",
    "css-loader": "^0.19.0",
    "daterangepicker": "0.0.3",
    "eslint": "1.5.1",
    "eslint-config-focus": "0.3.0",
    "eslint-plugin-filenames": "0.1.1",
    "eslint-plugin-react": "3.5.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "highlight.js": "^8.8.0",
    "jquery": "^2.1.4",
    "jsdom": "^6.4.0",
    "json-loader": "^0.5.3",
    "lunr": "^0.5.12",
    "mocha": "^2.2.5",
    "node-dir": "^0.1.9",
    "node-sass": "^3.3.3",
    "react-ace": "git+https://github.com/Bernardstanislas/react-ace.git",
    "react-addons-test-utils": "^0.14.0-rc1",
    "react-bootstrap-daterangepicker": "git+https://github.com/pierr/react-bootstrap-daterangepicker.git",
    "react-date-picker": "^3.1.6",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^2.0.1",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0",
    "webpack-focus": "^0.6.3"
  }
}
